{
  "name": "md-to-html",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "ava",
    "lint": "tslint --config tslint.json --project tsconfig.json --exclude package.json",
    "format": "prettier --config .prettierrc.js --write lib/**/*.{ts,tsx}",
    "check_format": "prettier --config .prettierrc.js --write lib/**/*.{ts,tsx} && git diff --exit-code",
    "clean": "rimraf dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kubosho/md-to-html.git"
  },
  "keywords": [
    "Markdown",
    "HTML",
    "Converter"
  ],
  "author": "kubosho",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kubosho/md-to-html/issues"
  },
  "homepage": "https://github.com/kubosho/md-to-html#readme",
  "devDependencies": {
    "@types/globby": "^8.0.0",
    "@types/node": "10.5.6",
    "@types/showdown": "1.7.5",
    "@types/yargs": "^11.1.1",
    "ava": "1.0.0-beta.6",
    "prettier": "1.14.0",
    "rimraf": "2.6.2",
    "ts-node": "7.0.0",
    "tslint": "5.11.0",
    "typescript": "3.0.1"
  },
  "dependencies": {
    "chrome-launcher": "^0.10.2",
    "globby": "^8.0.1",
    "option-t": "^18.0.0",
    "showdown": "^1.8.6",
    "yargs": "^12.0.1"
  },
  "ava": {
    "files": [
      "__tests__/test_*.*",
      "lib/__tests__/test_*.*",
      "lib/**/__tests__/test_*.*"
    ],
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "babel": {
      "extensions": [
        "js",
        "jsx"
      ],
      "testOptions": {
        "babelrc": false
      }
    }
  }
}
